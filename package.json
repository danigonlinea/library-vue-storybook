{
  "name": "watson-vue",
  "version": "0.0.23",
  "private": false,
  "description": "Watson Vue Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/danigonlinea/watson-vue-storybook"
  },
  "author": "danigonlinea",
  "main": "dist/watson-vue.umd.min.js",
  "license": "MIT",
  "files": [
    "dist/*",
    "src/styles/*"
  ],
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write .",
    "create:lib": "rm -rf dist && vue-cli-service build --no-clean --target lib --name watson-vue src/main.js",
    "release:patch": "yarn version --patch && yarn create:lib && yarn publish"
  },
  "sideEffects": true,
  "peerDependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/vue": "^6.4.19",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "sass-loader": "^10",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14"
  }
}
